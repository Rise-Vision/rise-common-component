  "dependencies": {
    "@polymer/polymer": "3.1.0",
    "@polymer/test-fixture": "~4.0.2",
    "@webcomponents/webcomponentsjs": "~2.1.3",
    "chai": "~4.2.0",
    "coveralls": "~3.0.6",
    "mocha": "~5.2.0",
    "sinon": "~7.3.2",
    "wct-istanbul": "~0.14.3",
    "wct-mocha": "~1.0.0"
